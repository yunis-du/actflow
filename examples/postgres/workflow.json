{
    "id": "postgres",
    "name": "Postgres Workflow",
    "desc": "A postgres workflow example",
    "env": {},
    "nodes": [
        {
            "id": "n1",
            "title": "start node",
            "desc": "start node",
            "uses": "start",
            "error_strategy": "none",
            "action": {}
        },
        {
            "id": "n2",
            "title": "http request",
            "desc": "http request action",
            "uses": "http_request",
            "error_strategy": "none",
            "action": {
                "url": "https://httpbin.org/get",
                "method": "GET",
                "auth": {
                    "auth_type": "no_auth"
                },
                "headers": {},
                "params": {},
                "body": {
                    "content_type": "none"
                },
                "timeout": 30000
            }
        },
        {
            "id": "n3",
            "title": "code",
            "desc": "code action",
            "uses": "code",
            "error_strategy": "none",
            "action": {
                "variables": [
                    {
                        "variable": "response",
                        "value_selector": "{{#n2.body#}}"
                    }
                ],
                "code_language": "javascript",
                "code": "function main(params) {\n    const body = params.response;\n    return { origin: body.origin, url: body.url };\n}"
            }
        }
    ],
    "edges": [
        {
            "id": "e1",
            "source": "n1",
            "target": "n2",
            "source_handle": "source"
        },
        {
            "id": "e2",
            "source": "n2",
            "target": "n3",
            "source_handle": "source"
        }
    ]
}